pro conversions

; Define conversion factors

; hPa/mb to Pa
defsysv, '!kilometer_by_meter', 1.d / 1000.d
defsysv, '!hpa_by_pascal', 1000d / 1.d5
defsysv, '!gram_by_kilogram', 1d3 / 1d
defsysv, '!centimeter_by_meter', 100d / 1d
defsysv, '!meter_by_micrometer',1d / 1d6
defsysv, '!micrometer_by_meter', 1d6 / 1d

end